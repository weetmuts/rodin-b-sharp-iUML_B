               org.eclipse.emf.edit.provider.ITableItemLabelProvider
               org.eclipse.emf.edit.provider.ITableItemColorProvider
               org.eclipse.emf.edit.provider.IItemColorProvider"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <extender
            uri="http://soton.ac.uk/models/eventb/diagrams"
            class="ac.soton.eventb.statemachines.provider.StatemachinesItemProviderAdapterFactory$DiagramsChildCreationExtender"/>
      <extender
            uri="http://emf.eventb.org/models/core/machine/2014"
            class="ac.soton.eventb.statemachines.provider.StatemachinesItemProviderAdapterFactory$CoreChildCreationExtender"/>
