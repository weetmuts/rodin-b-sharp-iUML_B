<?xml version="1.0" encoding="UTF-8"?>
<gmftool:ToolRegistry
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <palette
      title="Statemachine Palette">
    <tools
        xsi:type="gmftool:ToolGroup"
        title="States"
        collapsible="true">
      <tools
          xsi:type="gmftool:CreationTool"
          title="State"
          description="Create new State">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/icons/png/State.png"
            bundle="ac.soton.eventb.statemachines.edit"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Initial"
          description="Create new Initial State">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/icons/png/Initial.png"
            bundle="ac.soton.eventb.statemachines.edit"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Final"
          description="Create new Final State">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/icons/png/Final.png"
            bundle="ac.soton.eventb.statemachines.edit"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Any"
          description="Create new 'Any' pseudo-state for transitions from any source state">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/icons/png/ANY.png"
            bundle="ac.soton.eventb.statemachines.edit"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Junction"
          description="Create new 'Junction' pseudo-state for composing transitions from/to several source/target states">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/icons/png/Junction.png"
            bundle="ac.soton.eventb.statemachines.edit"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Fork/Join"
          description="Create new 'Fork/Join' pseudo-state for composing parallel transitions to/from several nested state machines">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/icons/png/Fork.png"
            bundle="ac.soton.eventb.statemachines.edit"/>
      </tools>
    </tools>
    <tools
        xsi:type="gmftool:ToolGroup"
        title="Features"
        collapsible="true">
      <tools
          xsi:type="gmftool:CreationTool"
          title="Statemachine"
          description="Create new Statemachine">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/icons/png/Statemachine.png"
            bundle="ac.soton.eventb.statemachines.edit"/>
      </tools>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Invariant"
          description="Create new Invariant">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/icons/png/Invariant.png"
            bundle="ac.soton.eventb.statemachines.edit"/>
      </tools>
    </tools>
    <tools
        xsi:type="gmftool:ToolGroup"
        title="Connections"
        collapsible="true">
      <tools
          xsi:type="gmftool:CreationTool"
          title="Transition"
          description="Create new Transition">
        <smallIcon
            xsi:type="gmftool:BundleImage"
            path="/icons/png/Transition.png"
            bundle="ac.soton.eventb.statemachines.edit"/>
      </tools>
    </tools>
  </palette>
</gmftool:ToolRegistry>
